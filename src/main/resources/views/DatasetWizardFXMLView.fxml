<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<VBox xmlns:fx="http://javafx.com/fxml/1" alignment="CENTER" prefHeight="700.0" prefWidth="800.0" spacing="15.0"
      xmlns="http://javafx.com/javafx/8.0.202" fx:controller="gui.controller.DatasetWizardController">
    <children>
        <Label text="Dataset/Cross Validation Setting">
            <font>
                <Font name="System Bold" size="18.0"/>
            </font>
        </Label>
        <ScrollPane fitToHeight="true" fitToWidth="true" prefHeight="447.0" prefWidth="770.0" VBox.vgrow="SOMETIMES">
            <content>
                <VBox fx:id="datasetsVbox" alignment="CENTER" maxHeight="1.7976931348623157E308"
                      maxWidth="1.7976931348623157E308" prefHeight="406.0" prefWidth="768.0" styleClass="box-style-1"
                      stylesheets="@../styles/stylesheet.css">
                    <children>
                        <HBox alignment="CENTER" prefHeight="43.0" prefWidth="729.0" spacing="15.0"
                              styleClass="box-style-1" stylesheets="@../styles/stylesheet.css">
                            <children>
                                <Button mnemonicParsing="false" onAction="#onNewDataSourceAction" prefHeight="25.0"
                                        prefWidth="173.0" text="Add New Data Source"/>
                                <Button mnemonicParsing="false" onAction="#onRemoveAction" prefHeight="25.0"
                                        prefWidth="173.0" text="Remove"/>
                                <Button mnemonicParsing="false" onAction="#onRemoveAction" prefHeight="25.0"
                                        prefWidth="173.0" text="Data Format Guideline"/>
                            </children>
                        </HBox>
                    </children>
                </VBox>
            </content>
        </ScrollPane>
        <VBox alignment="CENTER" minHeight="165.0" minWidth="770.0" spacing="5.0" styleClass="box-style-1"
              stylesheets="@../styles/stylesheet.css" VBox.vgrow="NEVER">
            <children>
                <GridPane>
                    <columnConstraints>
                        <ColumnConstraints hgrow="SOMETIMES" maxWidth="363.0" minWidth="10.0" prefWidth="204.0"/>
                        <ColumnConstraints hgrow="SOMETIMES" maxWidth="533.0" minWidth="10.0" prefWidth="532.0"/>
                    </columnConstraints>
                    <rowConstraints>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                    </rowConstraints>
                    <children>
                        <Label text="Number of Folds:"/>
                        <CheckBox fx:id="randomizationSeedCheckBox" mnemonicParsing="false"
                                  text="Set Randomization Seed" GridPane.rowIndex="1"/>
                        <CheckBox fx:id="runAlgorithmParallelCheckBox" mnemonicParsing="false"
                                  text="Run Algorithms Parallel?" GridPane.rowIndex="2"/>
                        <CheckBox fx:id="runFoldsParallelCheckBox" mnemonicParsing="false" text="Run Folds Parallel?"
                                  GridPane.rowIndex="3"/>
                        <TextField fx:id="randomizationSeedTextField" prefHeight="25.0" prefWidth="379.0"
                                   GridPane.columnIndex="1" GridPane.rowIndex="1"/>
                        <TextField fx:id="runAlgorithmNumberOfThreadTextField"
                                   promptText="If you leave this element empty,maximum number of cores will be used"
                                   GridPane.columnIndex="1" GridPane.rowIndex="2">
                            <tooltip>
                                <Tooltip text="If you leave this element empty,maximum number of cores will be used"/>
                            </tooltip>
                        </TextField>
                        <TextField fx:id="runFoldsNumberOfThreadTextField"
                                   promptText="If you leave this element empty,maximum number of cores will be used"
                                   GridPane.columnIndex="1" GridPane.rowIndex="3">
                            <tooltip>
                                <Tooltip text="If you leave this element empty,maximum number of cores will be used"/>
                            </tooltip>
                        </TextField>
                        <HBox alignment="CENTER" spacing="10.0" GridPane.columnIndex="1">
                            <children>
                                <Slider fx:id="numberOfFoldsSlider" blockIncrement="1.0" majorTickUnit="5.0" max="50.0"
                                        min="2.0" minorTickCount="4" showTickLabels="true" showTickMarks="true"
                                        snapToTicks="true" value="2.0" HBox.hgrow="ALWAYS"/>
                                <Label fx:id="numberOfFoldsLabel" alignment="TOP_CENTER" contentDisplay="TOP"
                                       prefHeight="47.0" prefWidth="53.0" text="Label" textAlignment="CENTER"
                                       textOverrun="CENTER_ELLIPSIS" HBox.hgrow="SOMETIMES">
                                    <font>
                                        <Font size="18.0"/>
                                    </font>
                                </Label>
                            </children>
                        </HBox>
                    </children>
                </GridPane>
            </children>
        </VBox>
    </children>
    <padding>
        <Insets bottom="15.0" left="15.0" right="15.0" top="15.0"/>
    </padding>
</VBox>
